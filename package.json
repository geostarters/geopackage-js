{
  "name": "geopackage",
  "version": "1.0.8",
  "description": "geopackage",
  "keywords": [
    "NGA",
    "mapcache"
  ],
  "license": "MIT",
  "author": {
    "name": "NGA",
    "url": "https://www.nga.mil"
  },
  "contributors": [
    "Daniel Barela <daniel.barela@gmail.com>"
  ],
  "homepage": "http://ngageoint.github.io/geopackage-js/",
  "repository": {
    "type": "git",
    "url": "https://github.com/ngageoint/geopackage-js.git"
  },
  "bugs": {
    "url": "https://github.com/ngageoint/geopackage-js/issues"
  },
  "bin": "./cli",
  "dependencies": {
    "async": "^1.5.0",
    "file-type": "^3.8.0",
    "proj4": "^2.3.17",
    "reproject": "^1.1.1",
    "sql.js": "^0.3.2",
    "stream-to-array": "^2.3.0",
    "wkx": "^0.2.0",
    "geojson-to-geopackage": "file:converters/geojson",
    "shapefile-to-geopackage": "file:converters/shapefile",
    "mbtiles-to-geopackage": "file:converters/mbtiles",
    "pbf-to-geopackage": "file:converters/pbf"
  },
  "devDependencies": {
    "browserify": "^13.0.0",
    "browserify-css": "^0.9.1",
    "browserify-istanbul": "^2.0.0",
    "browserify-shim": "^3.8.12",
    "chai": "^3.5.0",
    "image-diff": "^1.5.1",
    "ink-docstrap": "^1.1.4",
    "istanbul": "^0.4.2",
    "jsdoc": "^3.4.0",
    "karma": "^0.13.22",
    "karma-browserify": "^5.0.3",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^0.2.2",
    "karma-coverage": "^0.5.5",
    "karma-mocha": "^0.2.1",
    "karma-ng-html2js-preprocessor": "^0.2.0",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-spec-reporter": "0.0.23",
    "mocha": "^3.0.2",
    "testling": "^1.7.1",
    "watchify": "^3.7.0",
    "webworkify": "^1.1.0"
  },
  "optionalDependencies": {
    "asciify-image": "0.0.8",
    "chalk": "^1.1.3",
    "inquirer": "^1.0.2",
    "lwip": "0.0.9",
    "sqlite3": "3.1.2"
  },
  "private": true,
  "main": "index.js",
  "files": [
    "index.js",
    "static/geopackage.js",
    "cli",
    "lib",
    "converters"
  ],
  "browser": {
    "lwip": false,
    "sqlite3": false
  },
  "testling": {
    "harness": "mocha",
    "files": "test/*.js",
    "browsers": [
      "ie/6..latest",
      "chrome/22..latest",
      "firefox/16..latest",
      "safari/latest",
      "opera/11.0..latest",
      "iphone/6",
      "ipad/6",
      "android-browser/latest"
    ]
  },
  

  "scripts": {
    "test": "istanbul cover  _mocha",
    "mocha": "mocha",
    "karma": "karma start",
    "btest": "browserify ./test/test.js --standalone tests --exclude sqlite3 --exclude lwip --exclude inquirer -o bundle/test.bundle.js",
    "btestwatch": "watchify ./test/test.js -d -v --standalone tests --exclude sqlite3 --exclude lwip --exclude inquirer -o bundle/test.bundle.js",
    "predoc": "rm -rf docs/jsdoc/*",
    "doc": "jsdoc --verbose -c jsdoc.conf",
    "build": "browserify -t browserify-css geopackage.js --standalone geopackage --exclude sqlite3 --exclude lwip --exclude inquirer -o static/geopackage.js",
    "watch": "watchify -t browserify-css geopackage.js -v --standalone geopackage --exclude sqlite3 --exclude lwip --exclude inquirer -o static/geopackage.js"
  }
}
